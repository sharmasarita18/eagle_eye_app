pipeline {
    agent none
    stages {
         stage("git check out") {

             agent any

             steps {
                git 'https://github.com/sharmasarita18/eagle_eye_app.git'
             }

         }
        stage('Build image') {
            agent {
        dockerfile true
    }
            steps {
                echo 'Hello world!' 
                sh 'echo myname = $myname'
            }
        }
    }
}