pipeline {
    agent none
    stages {
         stage("git check out") {

             agent any

             steps {
                git branch: 'developer',
                credentialsId: '03ca8a43-fb79-4040-ad7a-127cfba2703b',
                url: 'https://github.com/sharmasarita18/eagle_eye_app.git'
            }

         }
        stage('Build image') {
            agent {
        dockerfile true
    }
            steps {
                echo 'Hello world!' 
                sh 'echo myname = $myname'
            }
        }
    }
}